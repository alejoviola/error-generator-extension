{
  "manifest_version": 3,
  "name": "Error Generator on client side",
  "version": "1.0.0",
  "description": "Generates errors on client side for testing purposes",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Error Generator"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
